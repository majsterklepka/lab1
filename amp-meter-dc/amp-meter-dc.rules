
(rules PCB amp-meter-dc
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (eu.mihosoft.freerouting.autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 126)
    (layer_rule B.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.9)
    )
  )
  (rule
    (width 800.0)
    (clear 350.2)
    (clear 400.0 (type smd_to_turn_gap))
    (clear 87.6 (type smd_smd))
  )
  (padstack "Via[0-1]_1000:635_um"
    (shape
      (circle B.Cu 1000.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_1000:635_um" "Via[0-1]_1000:635_um" default
  )
  (via 
    "Via[0-1]_1000:635_um-kicad_default" "Via[0-1]_1000:635_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_1000:635_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_1000:635_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 800.0)
    )
    (circuit 
      (use_layer B.Cu)
    )
  )
  (class "kicad_default"
    +12V GND "Net-(R1-Pad1)" "Net-(R2-Pad1)" "Net-(R3-Pad1)" "Net-(U1-Pad5)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 800.0)
    )
    (circuit 
      (use_layer B.Cu)
    )
  )
)