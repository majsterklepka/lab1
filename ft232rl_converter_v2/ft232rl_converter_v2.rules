
(rules PCB ft232rl_converter_v2
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 846)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.9)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.5)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-signal" "Via[0-1]_800:400_um" signal
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    signal "Via[0-1]_800:400_um-signal"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class signal
    GND VCC "Net-(C2-Pad1)" "Net-(C3-Pad1)" /VCCIO "Net-(D1-Pad1)" "Net-(D2-Pad1)" "Net-(J1-Pad2)"
    "Net-(J1-Pad3)" /TXD /CTS# /RXD /DCD# /DSR# /RI# /RTS#
    /DTR# /RXLED# /TXLED# /PWREN# "Net-(U1-Pad12)" "Net-(U1-Pad13)" "Net-(U1-Pad19)" "Net-(U1-Pad26)"
    "Net-(U1-Pad27)" "Net-(U1-Pad28)"
    (clearance_class signal)
    (via_rule signal)
    (rule
      (width 350.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)